{
  "name": "ubooze-backend",
  "version": "1.0.0",
  "description": "Server and scraper for alcohol price comparison webapp",
  "main": "index.js",
  "scripts": {
    "start": "node public/app.js",
    "dev-server": "nodemon --ignore '*.json' -r dotenv/config src/server.js",
    "client": "npm run dev --prefix client",
    "server": "nodemon -r dotenv/config src/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "tsc -p ."
  },
  "author": "Jools Barnett",
  "license": "MIT",
  "devDependencies": {
    "@types/cheerio": "^0.22.21",
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.23",
    "@types/puppeteer": "^3.0.1",
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "cheerio": "^1.0.0-rc.3",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "currency.js": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.2",
    "objection": "^2.2.1",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.3.0",
    "puppeteer": "^5.2.0",
    "puppeteer-autoscroll-down": "^0.1.5"
  }
}
